.container{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.header {
  display: flex;
  width: 100%;
  box-sizing: border-box;
  height: 100rpx;
  padding: 0 30rpx;
  /* 主轴的对齐方式，space-between两端对齐，中间等距分 */
  justify-content: space-between;
  align-items: center;
}

.date {
  align-items: baseline;
}

.header-right {
  display: inline-flex;
}

.hert {
  margin-right: 32rpx;
}


